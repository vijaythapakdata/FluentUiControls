{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "fluent-ui-controls-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/fluentUiControls/FluentUiControlsWebPart.js",
          "manifest": "./src/webparts/fluentUiControls/FluentUiControlsWebPart.manifest.json"
        }
      ]
    },
    "simple-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/simpleForm/SimpleFormWebPart.js",
          "manifest": "./src/webparts/simpleForm/SimpleFormWebPart.manifest.json"
        }
      ]
    },
    "crud-using-class-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/crudUsingClass/CrudUsingClassWebPart.js",
          "manifest": "./src/webparts/crudUsingClass/CrudUsingClassWebPart.manifest.json"
        }
      ]
    },
    "share-point-crud-api-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sharePointCrudApi/SharePointCrudApiWebPart.js",
          "manifest": "./src/webparts/sharePointCrudApi/SharePointCrudApiWebPart.manifest.json"
        }
      ]
    },
    "handling-large-list-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/handlingLargeList/HandlingLargeListWebPart.js",
          "manifest": "./src/webparts/handlingLargeList/HandlingLargeListWebPart.manifest.json"
        }
      ]
    },
    "formik-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/formikForm/FormikFormWebPart.js",
          "manifest": "./src/webparts/formikForm/FormikFormWebPart.manifest.json"
        }
      ]
    },
    "persona-card-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/personaCard/PersonaCardWebPart.js",
          "manifest": "./src/webparts/personaCard/PersonaCardWebPart.manifest.json"
        }
      ]
    },
    "spfxhttpclient-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spfxhttpclient/SpfxhttpclientWebPart.js",
          "manifest": "./src/webparts/spfxhttpclient/SpfxhttpclientWebPart.manifest.json"
        }
      ]
    },
    "spfx-aad-http-client-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spfxAadHttpClient/SpfxAadHttpClientWebPart.js",
          "manifest": "./src/webparts/spfxAadHttpClient/SpfxAadHttpClientWebPart.manifest.json"
        }
      ]
    },
    "spfx-ant-table-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/spfxAntTable/SpfxAntTableWebPart.js",
          "manifest": "./src/webparts/spfxAntTable/SpfxAntTableWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "FluentUiControlsWebPartStrings": "lib/webparts/fluentUiControls/loc/{locale}.js",
    "SimpleFormWebPartStrings": "lib/webparts/simpleForm/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "CrudUsingClassWebPartStrings": "lib/webparts/crudUsingClass/loc/{locale}.js",
    "SharePointCrudApiWebPartStrings": "lib/webparts/sharePointCrudApi/loc/{locale}.js",
    "HandlingLargeListWebPartStrings": "lib/webparts/handlingLargeList/loc/{locale}.js",
    "FormikFormWebPartStrings": "lib/webparts/formikForm/loc/{locale}.js",
    "PersonaCardWebPartStrings": "lib/webparts/personaCard/loc/{locale}.js",
    "SpfxhttpclientWebPartStrings": "lib/webparts/spfxhttpclient/loc/{locale}.js",
    "SpfxAadHttpClientWebPartStrings": "lib/webparts/spfxAadHttpClient/loc/{locale}.js",
    "SpfxAntTableWebPartStrings": "lib/webparts/spfxAntTable/loc/{locale}.js"
  }
}
